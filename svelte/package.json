{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "ENVIRONMENT=prod rollup -c",
    "dev": "ENVIRONMENT=dev rollup -c -w",
    "start": "sirv public --no-clear",
    "api": "rm -rf gen/api && openapi-generator generate -i  ../api/openapi.json --generator-name typescript-axios -o gen/api --config api.json && cd gen/api && npm install && npm run build"  
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@tsconfig/svelte": "^1.0.12",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.6.0",
    "svelte-preprocess": "^4.7.3",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "axios": "^0.21.1",
    "bootstrap": "^5.0.1",
    "rollup-plugin-copy": "^3.4.0",
    "sirv-cli": "^1.0.0"
  }
}
